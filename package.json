  "dependencies": {
    "bson": "^6.10.3",
    "type-fest": "^4.39.1"
  },
  "devDependencies": {
    "@kikiutils/changelogen": "^0.8.0",
    "@kikiutils/eslint-config": "^0.12.2",
    "@kikiutils/tsconfigs": "^4.1.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "vue": "^3.5.13"
  }
}
