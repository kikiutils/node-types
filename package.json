{
  "name": "@kikiutils/types",
  "version": "5.2.0",
  "description": "Lightweight utility types package with global declarations, Vue support, and no import required.",
  "author": "kiki-kanri",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kikiutils/node-types.git"
  },
  "keywords": [
    "component-types",
    "declaration",
    "declare-global",
    "global-types",
    "no-import",
    "ts",
    "type-fest",
    "types",
    "typescript",
    "utility-types",
    "vue",
    "vue-types"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts"
    },
    "./vue": {
      "types": "./dist/vue.d.ts"
    }
  },
  "files": [
    "./dist"
  ],
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "bumplog": "changelogen --bump --hideAuthorEmail",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepack": "pnpm run build",
    "release": "pnpm run lint && pnpm run test && pnpm run build && changelogen --hideAuthorEmail --push --release && npm publish",
    "test": "tsc -p ./tsconfig.test-check.json && jest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "bson": "^6.10.3",
    "type-fest": "^4.40.1"
  },
  "devDependencies": {
    "@kikiutils/changelogen": "^0.8.0",
    "@kikiutils/eslint-config": "^0.12.4",
    "@kikiutils/tsconfigs": "^5.0.1",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.2",
    "typescript": "^5.8.3",
    "vue": "^3.5.13"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "unrs-resolver"
    ]
  }
}
