{
  "name": "@kikiutils/types",
  "version": "5.0.0",
  "description": "A lightweight utility types package that globally provides commonly used, simplified, and Vue-specific types â€” with zero import hassle.",
  "author": "kiki-kanri",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kikiutils/node-types.git"
  },
  "keywords": [
    "declare-global",
    "global-types",
    "ts",
    "type-fest",
    "types",
    "typescript",
    "typings",
    "utility-types",
    "vue",
